{
    "env": {
        "development": {
            "plugins": [
                "transform-class-properties"
            ],
            "presets": [
                "es2015",
                "react",
                "stage-2"
            ]
        },

        "production": {
            "plugins": [
                "external-helpers",
                "transform-class-properties"
            ],
            "presets": [
                ["es2015", {"modules": false}],
                "react",
                "stage-2"
            ],
        },

        "test": {
            "compact": true,
            "comments": false,
            "plugins": [
                "transform-class-properties"
            ],
            "presets": [
                "es2015",
                "react",
                "stage-2"
            ]
        }
    }
}
